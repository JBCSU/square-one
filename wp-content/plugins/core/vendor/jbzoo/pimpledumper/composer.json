{
    "name"              : "jbzoo/pimpledumper",
    "type"              : "library",
    "description"       : "Easy way to auto create pimple.json and .phpstorm.meta.php for Pimple Container",
    "license"           : "MIT",
    "keywords"          : ["pimple", "silex", "idea", "phpstorm", "autocomplete", "arrayaccess", "di", "container"],
    "minimum-stability" : "stable",
    "authors"           : [
        {
            "name"  : "SmetDenis",
            "email" : "denis@jbzoo.com",
            "role"  : "lead"
        }
    ],
    "require"           : {
        "php" : ">=5.3.10"
    },
    "require-dev"       : {
        "pimple/pimple" : "^3.0",
        "jbzoo/phpunit" : "^1.7|1.x-dev"
    },
    "suggest"           : {
        "pimple/pimple" : "^3.0"
    },
    "autoload"          : {
        "psr-4" : {
            "JBZoo\\PimpleDumper\\" : "src"
        }
    },
    "extra"             : {
        "branch-alias" : {
            "dev-master" : "1.x-dev"
        }
    },
    "config"            : {
        "optimize-autoloader" : true
    },
    "support"           : {
        "issues" : "https://github.com/JBZoo/PimpleDumper/issues"
    }
}

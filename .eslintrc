{
  "extends": "airbnb",
  "rules": {
    "import/prefer-default-export": 0,
    "no-useless-escape": 0,
    "no-tabs": 0,
    "indent": [
      "error",
      "tab"
    ],
    "max-len": [
      "error",
      500,
      4
    ],
    "no-console": 0,
    "no-new": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "jquery": true
  }
}
